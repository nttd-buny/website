# {{ ansible_managed }}

{% for key,value in nginx_conf.iteritems() | sort %}
{{ key }} {{ value }};
{% endfor %}

events {
{% for key, value in nginx_events.iteritems() | sort %}
    {{ key }} {{ value }};
{% endfor %}
}

http {
{% for key, value in nginx_http.iteritems() | sort %}
    {{ key }} {{ value }};
{% endfor %}

    include /etc/nginx/conf.d/*.conf;
    include /etc/nginx/sites-enabled/*;
}
